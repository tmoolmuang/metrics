<div class="row">
  <div class="col-md-1">
  </div>
  <div class="col-md-10 top">
    <h4><%= @event.name %></h4>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <% if event_owner? || admin? %>
      <%= link_to "Edit Event", edit_app_event_path(@event.app, @event), class: 'btn btn-primary' %>
      <%= link_to "Delete Event", [@event.app, @event], method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this event?' } %>
    <% end %>
    <%= link_to "Back to Application", app_path(@event.app), class: 'btn btn-default' %>
  </div>
  <div class="col-md-1">
  </div>
</div>
<div class="row">
  <div class="col-md-1">
  </div>
  <div class="col-md-10 mid">
    <p>*<%= @event.name %>*</p>
  </div>
  <div class="col-md-1">
  </div>
</div>